<div class="post-header-line-1">	<span class="post-author vcard">Author:<span class="fn"> {{$user_name}}</span></span>	<span class="post-timestamp"> at<abbr class="published"title=""> {{date $created "2006.01.02 15:04:05"}}</abbr></span>	{{if $tags}} |		<span class="post-labels">Tags:		{{range $tags}}			<a href="/tag/{{.slug}}" rel="tag">{{.name}}({{.count}})</a>,		{{end}}		</span>	{{else}} |	<span class="post-labels">No tags.</span>	{{end}}</div>